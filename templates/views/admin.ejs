<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/admin.css">
    <link rel="stylesheet" href="/css/form.css">

    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <!-- <link rel="stylesheet" href="/css/dashboard.css"> -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <title>Admin</title>
</head>

<body>
    <section id="set">
        <div id="set-add">
            <!-- <input type="text" name="set" id="setname"> -->
            <div class="container">
                <label>Sample Multi Data Select Box</label>
                <br/><br/>
                  <select name="" multiple id="addiction">
                      <% arr.forEach(item=>{ %>
                        <option><%= item.name %></option>
                      <% }) %>
                  </select>
              </div>
            <div class="slider">
            <input type = "range" id="strength" min="0" max="100" onchange="rangevalue.value=value"/>
            <output id="rangevalue">50</output>
            </div>
            <input type="number" name="time" id="time">
            <button onclick="postSet()">Submit</button>
        </div>
        <div id="add-activity">
            <input type="text" name="name" id="activity-name">
            <input type="text" name="description" id="activity-description">
            <button onclick="postActivity()">Submit</button>
        </div>
    </section>
    <div class="projects-section">
        <div class="projects-section-header">
            <p>Total Activities</p>
        </div>
        <div class="search-wrapper">
            <input class="search-input" id="search-input" type="text" placeholder="Search">
            <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" fill="none" stroke="currentColor"
                stroke-linecap="round" stroke-linejoin="round" stroke-width="2" class="feather feather-search"
                viewBox="0 0 24 24">
                <defs></defs>
                <circle cx="11" cy="11" r="8"></circle>
                <path d="M21 21l-4.35-4.35"></path>
            </svg>
        </div>
        <div class="projects-section-line">
            <div class="projects-status">
                <div class="item-status">
                        <!-- <span class="status-number">62</span>
                        <span class="status-type">Total Projects</span> -->
                    
                </div>
            </div>

        </div>
        <div class="project-boxes jsGridView" id="project-boxes">

        </div>
    </div>
    </div>
    </div>
</body>
<script src="/javascript/admin.js"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p"
    crossorigin="anonymous"></script>

</html>